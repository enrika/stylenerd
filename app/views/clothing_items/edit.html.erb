<h1> Edit Clothing Item</h1>
 
  <%= form_for :clothing_item, url: clothing_items_path(@clothing), method: :patch do |f| %>
 
 <p>
    <%= f.label "Clothing Type" %><br>
    <%= f.text_field :clothing_type %>
  </p>
 

  <p>
    <%= f.label "Nick Name or Short Description" %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :color %><br>
    <%= f.text_field :color %>
  </p>

  
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', clothing_items_path %>